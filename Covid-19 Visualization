   "source": [
    "# india daily cases\n",
    "\n",
    "ind_df['Daily Confirmed'] = ind_df['Confirmed'].sub(ind_df['Confirmed'].shift())\n",
    "ind_df['Daily Death'] = ind_df['Deaths'].sub(ind_df['Deaths'].shift())\n",
    "ind_df['Daily Recovered'] = ind_df['Recovered'].sub(ind_df['Recovered'].shift())\n",
    "fig = plt.figure(figsize=(20,8))\n",
    "ax= fig.add_subplot(111)\n",
    "\n",
    "ax.bar(ind_df['Date'], ind_df['Daily Confirmed'],color='b', label='India daily confirmed cases')\n",
    "\n",
    "ax.bar(ind_df['Date'], ind_df['Daily Recovered'],color='g', label='India daily recovered')\n",
    "ax.bar(ind_df['Date'], ind_df['Daily Death'],color='r', label='India daily deaths')\n",
    "ax.set_xlabel('Date')\n",
    "ax.set_ylabel('# pf people affected')\n",
    "ax.title.set_text('India daily cases and deaths')\n",
    "\n",
    "plt.show()"
   ]
