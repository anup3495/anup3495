    "ind_df=df[df['Country']=='India'].groupby(['Date']).sum()\n",
    "us_df=df[df['Country']=='US'].groupby(['Date']).sum()\n",
    "brazil_df=df[df['Country']=='Brazil'].groupby(['Date']).sum()\n",
    "UK_df=df[df['Country']=='United Kingdom'].groupby(['Date']).sum()\n",
    "china_df=df[df['Country']=='China'].groupby(['Date']).sum()\n",
    "fig = plt.figure(figsize=(20,10))\n",
    "ax= fig.add_subplot(111)\n",
    "\n",
    "ax.plot(world_df[['Total Cases']],label='Worldwide')\n",
    "ax.plot(ind_df[['Total Cases']],label='India')\n",
    "ax.plot(us_df[['Total Cases']],label='US')\n",
    "ax.plot(brazil_df[['Total Cases']],label='Brazil')\n",
    "ax.plot(UK_df[['Total Cases']],label='United Kingdom')\n",
    "ax.plot(china_df[['Total Cases']],label='China')\n",
    "\n",
    "\n",
    "ax.set_xlabel('Date')\n",
    "ax.set_ylabel('# of cases')\n",
    "ax.title.set_text('World vs India total cases')\n",
    "plt.legend(loc='upper left')\n",
    "plt.show()"
   ]
